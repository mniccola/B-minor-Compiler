// four loops

main: function integer () = {
    i: integer;
    d: integer = 10;
    for(i = 0; i < d; i++) {
        print i, "\n";
    }


    i = 0;
    for(;i < d; i++) {
        print i, "\n";
    }

    i = 0;
    for(i = 0; i < d;) {
        print i, "\n";
        i = i + 1;
    }

    i = 0;
    for( ; ; ) {
        if(i > 20) {
            return i;
        }
        print i, "\n";
        i = i + 1;
    }
    

    return 0;
}